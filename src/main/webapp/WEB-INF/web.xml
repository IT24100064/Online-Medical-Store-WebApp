<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                             http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <display-name>Feedback &amp; Review Management</display-name>




    <servlet>
    <servlet-name>AdminCreateServlet</servlet-name>
    <servlet-class>com.onlinepharmacy.servlet.AdminCreateServlet</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>AdminCreateServlet</servlet-name>
<url-pattern>/adminCreate</url-pattern>
</servlet-mapping>

<servlet>
<servlet-name>AdminListServlet</servlet-name>
<servlet-class>com.onlinepharmacy.servlet.AdminListServlet</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>AdminListServlet</servlet-name>
<url-pattern>/adminList</url-pattern>
</servlet-mapping>

<servlet>
<servlet-name>AdminUpdateServlet</servlet-name>
<servlet-class>com.onlinepharmacy.servlet.AdminUpdateServlet</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>AdminUpdateServlet</servlet-name>
<url-pattern>/adminUpdate</url-pattern>
</servlet-mapping>

<servlet>
<servlet-name>AdminDeleteServlet</servlet-name>
<servlet-class>com.onlinepharmacy.servlet.AdminDeleteServlet</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>AdminDeleteServlet</servlet-name>
<url-pattern>/adminDelete</url-pattern>
</servlet-mapping>
    </web-app>
